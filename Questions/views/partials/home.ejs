<div>
	<a href="./">Home</a> | <a href="#/new_question">Add a Question</a>
	<div>
		{{message}}
	
	</div>
	<input type="text" name="search" ng-model="search">
	<table>
		<tr>
			<th>Question</th>
			<th>Answers</th>
			<th>Action</th>
		</tr>
		<tr ng-repeat="question in questions | filter: search ">
			<td>{{question.name}}</td>
			<td>{{question.answers}}</td>
			<td>{{question.action}}</td>
		</tr>
	</table>
</div>